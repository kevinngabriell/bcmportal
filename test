const subQuestion = mainQuestion.followUpsIfYes?.[0];

const followUp = subQuestion?.followUpsIfYes?.find(
  (item) =>
    item.type === "question" &&
    item.label.toLowerCase().includes("memenuhi seluruh standar")
);

const valueFollowUp = followUp
  ? resolveValueFromItem(followUp, rowClean, suffix)
  : null;