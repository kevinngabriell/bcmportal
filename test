export function aoaToJson(data: (string | null)[][]): JsonRow[] {
  return data
    .filter(row => row.length > 0 && row[0]) // skip row kosong
    .map(row => {
      if (row.length === 1 || !row[1]) {
        return {
          field: String(row[0]),
          value: null,
          type: "title"
        };
      } else {
        return {
          field: String(row[0]),
          value: row[1],
          type: "question"
        };
      }
    });
}